{
  "name": "smartoysters/yellowfin-rest-api",
  "description": "Yellowfin API client for PHP",
  "keywords": ["yellowfin", "api", "client"],
  "license": "MIT",
  "authors": [
    {
      "name": "James Rickard",
      "email": "james.rickard@smartoysters.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "guzzlehttp/guzzle": "~7.0|~6.0|~5.0|~4.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "SmartOysters\\Yellowfin\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "phpspec/phpspec": "^2.4",
    "symfony/var-dumper": "^2.7"
  }
}
